@model IEnumerable<BaseDatos.Models.Coche>
@{
    ViewBag.Title = "Indexo";
}

<h2>Indexo</h2>
<body>
    <div class="container-fluid py-4">
        <h4 class="text-center text-uppercase">Consulta</h4>
        <table class="table table-striped table-bordered">
            <thead class="bg-dark text-white">
                <tr>
                    <th>UsuarioCoche</th>
                  


                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Modelo.NombreModelo</td>
                     

                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
